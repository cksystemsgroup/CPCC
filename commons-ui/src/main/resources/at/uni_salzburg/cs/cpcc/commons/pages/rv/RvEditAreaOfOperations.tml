<l:LeafletMap t:id="map" t:drawControl="true" t:center="prop:mapCenter"
	t:zoom="prop:zoomLevel" t:height="500" />

<l:LeafletRegionLayer t:mapId="map"
	t:name="message:otherRealVehicles.name"
	t:value="prop:otherRealVehicleRegions" t:frequencySecs="15" />

<l:LeafletDrawControl t:id="drawControl" t:mapId="map"
	t:data="prop:realVehicleRegions" t:position="topleft"
	t:polygonEnabled="true" t:rectangleEnabled="false"
	t:polylineEnabled="false" t:circleEnabled="false" />

