<html t:type="layout" title="${message:realVehicleIndex}"
	t:sidebarTitle="Framework Version" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
	xmlns:p="tapestry:parameter" xmlns:c="tapestry-library:commons">

	<div class="row">
		<div class="col-xs-6 col-md-3">
			<h4>
				<t:Pagelink page="index">${message:device.tree.label}
				</t:Pagelink>
			</h4>
			<t:If test="deviceList">
				<c:DeviceTree t:devices="deviceList" />
			</t:If>
		</div>
	</div>

	<div class="col-xs-12 col-md-9">
		<t:EventLink event="rvSync" class="btn btn-success">RV
			Sync
		</t:EventLink>&nbsp;&nbsp;
		<t:EventLink event="configSync" class="btn btn-success">Config
			Sync
		</t:EventLink>
	</div>

</html>
